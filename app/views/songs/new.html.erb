<h1>Create a Song</h1>

<% notes= [] %>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :artist_name%>
  <%= f.text_field :artist_name%>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>
  <%= f.fields_for :note_contents  do |notes_form| %>
    <%= notes_form.text_field "1", :id => "song_notes_1"%>
    <%= notes_form.text_field  "2", :id => "song_notes_2"%>
    <%= notes_form.text_field  "3", :id => "song_notes_3"%>
  <%end%> 
 

  <br />

  <%= f.submit %>
<% end %>


